<?xml version="1.0" encoding="utf-8"?>
<Theme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://microsoft.data.visualization.engine.Theme/1.0">
  
  <!-- Earthy -->

<!-- Default directional light -->
  <Lighting>
    <DirectionalLightPosition>
      <X>-.25</X>
      <Y>.5</Y>
      <Z>-1</Z>
    </DirectionalLightPosition>
    <PointLightPosition>
      <X>.25</X>
      <Y>-.5</Y>
      <Z>-.5</Z>
    </PointLightPosition>
    <PointLightAttenuationFactor>0.001</PointLightAttenuationFactor>
    <AmbientLightFactor>0.5</AmbientLightFactor>
    <SecondaryAmbientLightFactor>.5</SecondaryAmbientLightFactor>
    <DirectionalLightFactor>0.9</DirectionalLightFactor>
    <PointLightFactor>0.5</PointLightFactor>
  </Lighting>
  
  <!-- Background color is a linear gradient (top to bottom): RGB 177/193/200 to RGB 209/228/236 -->
  <BackgroundTopColor>
    <R>0.341176</R>
    <G>0.317647</G>
    <B>0.266666</B>
    <A>1</A>
  </BackgroundTopColor>
  <BackgroundBottomColor>
    <R>0.729412</R>
    <G>0.682352</G>
    <B>0.560784</B>
    <A>1</A>
  </BackgroundBottomColor>
    
  <GlobeGlowEnabled>true</GlobeGlowEnabled>
  <GlobeGlowColor>
    <R>1</R>
    <G>1</G>
    <B>1</B>
    <A>1</A>
  </GlobeGlowColor>
  <GlobeGlowReflectanceIndex>0.001</GlobeGlowReflectanceIndex>
  <GlobeGlowPower>7</GlobeGlowPower>
  <GlobeGlowFactor>0.5</GlobeGlowFactor>
  
  <!-- Globe treatment -->
  <ColorOperations>

    <ColorReplace>
      <Source>
        <!-- Water color -->
        <R>0.725490</R>
        <G>0.811765</G>
        <B>0.921569</B>
        <A>1</A>
      </Source>
      <Destination>
        <R>0.6</R>
        <G>0.6</G>
        <B>0.6</B>
        <A>1</A>
      </Destination>
    </ColorReplace>

    <SaturationTransform>
      <Saturation>0.7</Saturation>
    </SaturationTransform>

    <!-- Color fill (multiply) RGB 106/127/191 -->
    <MultiplyTransform>
      <Color>
        <R>0.85098</R>
        <G>0.815686</G>
        <B>0.709804</B>
        <A>1</A>
      </Color>
    </MultiplyTransform>

    
  </ColorOperations>

  <!-- Annotation gradient colors-->
  
  <AnnotationTopColor>
    <R>0.9725</R>
    <G>0.7255</G>
    <B>0.3569</B>
    <A>1</A>
  </AnnotationTopColor>
  <AnnotationBottomColor>
    <R>0.9647</R>
    <G>0.6549</G>
    <B>0.3020</B>
    <A>1</A>
  </AnnotationBottomColor>

  <!-- Shadow Color -->

  <ShadowColor>
    <R>0</R>
    <G>0</G>
    <B>0</B>
    <A>0.2</A>
  </ShadowColor>

  <!-- Outline parameters -->
  <LayerParameters>
    <InnerOutlineColor>
      <R>1</R>
      <G>1</G>
      <B>1</B>
      <A>1</A>
    </InnerOutlineColor>
    <OuterOutlineColor>
      <R>0</R>
      <G>0</G>
      <B>0</B>
      <A>1</A>
    </OuterOutlineColor>
    <OutlineWidth>2</OutlineWidth>
    <OutlineOffset>0</OutlineOffset>
  </LayerParameters>

  <!-- Data Color Steps, Orange first in SoDo order -->
<ColorsForVisuals>
    <Color>
      <R>0.333333</R>
      <G>0.729412</G>
      <B>0.733333</B>
      <A>1</A>
    </Color>
    <Color>
      <R>0.976471</R>
      <G>0.596078</G>
      <B>0.023529</B>
      <A>1</A>
    </Color>
    <Color>
      <R>0.686275</R>
      <G>0.796078</G>
      <B>0.152941</B>
      <A>1</A>
    </Color>
    <Color>
      <R>0.933333</R>
      <G>0.431373</G>
      <B>0.411765</B>
      <A>1</A>
    </Color>
    <Color>
      <R>1</R>
      <G>0.866667</G>
      <B>0.247059</B>
      <A>1</A>
    </Color>
    <Color>
      <R>0.52549</R>
      <G>0.482353</G>
      <B>0.639216</B>
      <A>1</A>
    </Color>
  </ColorsForVisuals>
  
  <ColorStepsForVisuals>
    <Step> 1.0 </Step>
    <Step> 1.5 </Step>
    <Step> 0.84 </Step>
    <Step> 1.32 </Step>
    <Step> 0.76 </Step>
    <Step> 1.08 </Step>
    <Step> 0.52 </Step>
    <Step> 1.16 </Step>
    <Step> 0.68 </Step>
    <Step> 1.32 </Step>
    <Step> 0.92 </Step>
    <Step> 1.48 </Step>
    <Step> 0.60 </Step>
  </ColorStepsForVisuals>

  <!--Visual Width Factor -->

  <InstanceVisualWidthFactor>
    0.8
  </InstanceVisualWidthFactor>
  
</Theme>