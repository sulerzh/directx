<?xml version="1.0" encoding="utf-8"?>
<Theme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://microsoft.data.visualization.engine.Theme/1.0">

  <!-- Color Black -->

  <!-- Default directional light -->
  <Lighting>
    <DirectionalLightPosition>
      <X>-.25</X>
      <Y>.5</Y>
      <Z>-1</Z>
    </DirectionalLightPosition>
    <PointLightPosition>
      <X>.25</X>
      <Y>-.5</Y>
      <Z>-.5</Z>
    </PointLightPosition>
    <PointLightAttenuationFactor>0.001</PointLightAttenuationFactor>
    <AmbientLightFactor>0.5</AmbientLightFactor>
    <SecondaryAmbientLightFactor>.5</SecondaryAmbientLightFactor>
    <DirectionalLightFactor>0.9</DirectionalLightFactor>
    <PointLightFactor>0.5</PointLightFactor>
  </Lighting>

  <!-- Background color is a linear gradient (top to bottom): RGB  to RGB  -->
  <BackgroundTopColor>
    <R>0.137254902</R>
    <G>0.137254902</G>
    <B>0.137254902</B>
    <A>1</A>
  </BackgroundTopColor>
  <BackgroundBottomColor>
    <R>0</R>
    <G>0</G>
    <B>00</B>
    <A>1</A>
  </BackgroundBottomColor>

  <GlobeGlowEnabled>true</GlobeGlowEnabled>
  <GlobeGlowColor>
    <R>1</R>
    <G>1</G>
    <B>1</B>
    <A>1</A>
  </GlobeGlowColor>
  <GlobeGlowReflectanceIndex>0.001</GlobeGlowReflectanceIndex>
  <GlobeGlowPower>9</GlobeGlowPower>
  <GlobeGlowFactor>0.15</GlobeGlowFactor>

  <!-- Globe treatment -->
  <ColorOperations>
    <InvertTransform/>
    <HueTransform>
      <HueRotation>180</HueRotation>
    </HueTransform>
    <SaturationTransform>
      <Saturation>0</Saturation>
    </SaturationTransform>



  </ColorOperations>

  <!-- Shadow Color -->
  <ShadowColor>
    <R>0</R>
    <G>0</G>
    <B>0</B>
    <A>0.2</A>
  </ShadowColor>

  <!-- Outline parameters -->
  <LayerParameters>
    <InnerOutlineColor>
      <R>1</R>
      <G>1</G>
      <B>1</B>
      <A>1</A>
    </InnerOutlineColor>
    <OuterOutlineColor>
      <R>0</R>
      <G>0</G>
      <B>0</B>
      <A>1</A>
    </OuterOutlineColor>
    <OutlineWidth>2</OutlineWidth>
    <OutlineOffset>0</OutlineOffset>
  </LayerParameters>

  <!-- Data Color Steps,  -->
  <ColorsForVisuals>
