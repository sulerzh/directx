<?xml version="1.0" encoding="utf-8"?>
<Theme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://microsoft.data.visualization.engine.Theme/1.0">
  
  <!-- Outline no data color -->

  <!-- Default directional light -->
  <Lighting>
    <DirectionalLightPosition>
      <X>-.25</X>
      <Y>.5</Y>
      <Z>-1</Z>
    </DirectionalLightPosition>
    <PointLightPosition>
      <X>.25</X>
      <Y>-.5</Y>
      <Z>-.5</Z>
    </PointLightPosition>
    <PointLightAttenuationFactor>0.001</PointLightAttenuationFactor>
    <AmbientLightFactor>0.5</AmbientLightFactor>
    <SecondaryAmbientLightFactor>.5</SecondaryAmbientLightFactor>
    <DirectionalLightFactor>0.9</DirectionalLightFactor>
    <PointLightFactor>0.5</PointLightFactor>
  </Lighting>
  
  <!-- Background color is a linear gradient (top to bottom): RGB 177/193/200 to RGB 209/228/236 -->
  <BackgroundTopColor>
    <R>0.32</R>
    <G>0.38</G>
    <B>0.55</B>
    <A>1</A>
  </BackgroundTopColor>
  <BackgroundBottomColor>
    <R>0.65</R>
    <G>0.75</G>
    <B>0.85</B>
    <A>1</A>
  </BackgroundBottomColor>
    
  <GlobeGlowEnabled>true</GlobeGlowEnabled>
  <GlobeGlowColor>
    <R>1</R>
    <G>1</G>
    <B>1</B>
    <A>1</A>
  </GlobeGlowColor>
  <GlobeGlowReflectanceIndex>0.001</GlobeGlowReflectanceIndex>
  <GlobeGlowPower>7</GlobeGlowPower>
  <GlobeGlowFactor>0.9</GlobeGlowFactor>
  
  <!-- Globe treatment -->
  <ColorOperations>
    
  </ColorOperations>

  <!-- Annotation style-->

  <AnnotationStyle>
    <TailDimensions>
      <!-- tail's X extent -->
      <X>38</X>
      <!-- tail's Y extent -->
      <Y>28</Y>
      <!-- tail's height -->
      <Z>17</Z>
    </TailDimensions>
    <OutlineWidth>1.414</OutlineWidth>
    <AnnotationScale>1</AnnotationScale>
    <OutlineColor>
      <R>0.527</R>
      <G>0.527</G>
      <B>0.527</B>
      <A>0.8</A>
    </OutlineColor>
    <FillColor>
      <R>1</R>
      <G>1</G>
      <B>1</B>
      <A>1</A>
    </FillColor>
  </AnnotationStyle>

  <!-- Shadow Color -->

  <ShadowColor>
    <R>0</R>
    <G>0</G>
    <B>0</B>
    <A>0.2</A>
  </ShadowColor>

  <!-- Outline parameters -->
  <LayerParameters>
    <InnerOutlineColor>
      <R>1</R>
      <G>1</G>
      <B>1</B>
      <A>1</A>
    </InnerOutlineColor>
    <OuterOutlineColor>
      <R>0</R>
      <G>0</G>
      <B>0</B>
      <A>1</A>
    </OuterOutlineColor>
    <OutlineWidth>2</OutlineWidth>
    <OutlineOffset>0</OutlineOffset>
  </LayerParameters>

  <!-- Data Color Steps, RED first in SoDo order -->
  <ColorsForVisuals>
    <Color>
      <R>0.133333</R>
      <G>0.411765</G>
      <B>0.690196</B>
      <A>1</A>
    </Color>
    <Color>
      <R>0.960784</R>
      <G>0.541176</G>
      <B>0.07451</B>
      <A>1</A>
    </Color>
    <Color>
      <R>0.478431</R>
      <G>0.141176</G>
      <B>0.690196</B>
      <A>1</A>
    </Color>
    <Color>
      <R>1</R>
     <G>0.760784</G>
      <B>0.109804</B>
      <A>1</A>
    </Color>
    <Color>
      <R>0.113725</R>
      <G>0.572549</G>
