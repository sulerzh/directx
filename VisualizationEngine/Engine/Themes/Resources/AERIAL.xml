    <Color>
      <R>0.992157</R>
      <G>0.231373</G>
      <B>0.07451</B>
      <A>1</A>
    </Color>


  </ColorsForVisuals>
  <ColorStepsForVisuals>
    <Step> 1.0 </Step>
    <Step> 1.5 </Step>
    <Step> 0.84 </Step>
    <Step> 1.32 </Step>
    <Step> 0.76 </Step>
    <Step> 1.08 </Step>
    <Step> 0.52 </Step>
    <Step> 1.16 </Step>
    <Step> 0.68 </Step>
    <Step> 1.32 </Step>
    <Step> 0.92 </Step>
