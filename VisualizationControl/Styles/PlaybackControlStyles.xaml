                                    To="{StaticResource InactiveColor}" Duration="0"/>
                    <ColorAnimation Storyboard.TargetName="LoopBrush" Storyboard.TargetProperty="Color"
                                    To="{StaticResource InactiveColor}" Duration="0"/>
                    <DoubleAnimation Storyboard.TargetName="LoopDrawingBrush" Storyboard.TargetProperty="Opacity" To="1" Duration="0"/>
                  </Storyboard>
                </BeginStoryboard>
              </TriggerBase.EnterActions>
              <TriggerBase.ExitActions>
                <StopStoryboard BeginStoryboardName="LoopingInactive"/>
              </TriggerBase.ExitActions>
            </MultiTrigger>
